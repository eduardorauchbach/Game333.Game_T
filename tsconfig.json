{
    "compilerOptions": {
        "target": "es2021",
        "module": "esnext",
        "lib": ["es2021", "dom"],
        "outDir": "./compiled",
        "rootDir": "./ts",
        "strict": true,
        "strictPropertyInitialization": false,
        "strictNullChecks": false
    },
    "include": ["./ts/**/*"],
    "tsc-alias": {
        "resolveFullPaths": true,
        "verbose": false
    }
}
